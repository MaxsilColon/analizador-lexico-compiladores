// Ejemplo 5: Operadores y comparaciones
int a = 10;
int b = 20;

boolean igual = (a == b);
boolean diferente = (a != b);
boolean menor = (a < b);
boolean mayor = (a > b);
boolean menorIgual = (a <= b);
boolean mayorIgual = (a >= b);

int suma = a + b;
int resta = a - b;
int multiplicacion = a * b;
int division = a / b;
